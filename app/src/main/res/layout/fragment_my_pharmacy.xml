<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ListView
        android:id="@+id/fragment_my_pharmacy_list_medicines"
        android:layout_width="373dp"
        android:layout_height="595dp"
        android:layout_margin="16dp"
        android:background="#144CAF50"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragment_my_pharmacy_lbl_medicines"
        app:layout_constraintVertical_bias="0.079" />

    <Button
        android:id="@+id/fragment_my_pharmacy_button_add_medicine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/fragment_my_pharmacy_button_add_medicines"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.917"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fragment_my_pharmacy_list_medicines" />

    <TextView
        android:id="@+id/fragment_my_pharmacy_lbl_medicines"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/fragment_my_pharmacy_lbl_medicines"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.053"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022" />
</androidx.constraintlayout.widget.ConstraintLayout>